{"ast":null,"code":"import MyPopup from '@/components/MyPopup.vue';\nimport MySearch from '@/components/MySearch.vue';\nimport MyTable from '@/components/MyTable.vue';\nimport MyPages from '@/components/MyPages.vue';\nexport default {\n  components: {\n    MySearch,\n    MyTable,\n    MyPages,\n    MyPopup\n  },\n\n  data() {\n    return {\n      popup: '',\n      showPopup: false,\n      tableHeader: [{\n        headerItem: ['#', 'Товар', 'Кол-во позиций', 'Поставщик', 'Стоимость']\n      }],\n      tableData: [{\n        Items: [{\n          tableItem: ['1', '5', 'Петр', '5000'],\n          id: 1\n        }, {\n          tableItem: ['2', '10', 'Иван', '10000'],\n          id: 2\n        }]\n      }],\n      value: '',\n      currentPage: 0\n    };\n  },\n\n  methods: {\n    setPage(currentPage) {\n      this.currentPage = currentPage;\n    },\n\n    showPopupToggle() {\n      if (this.showPopup == true) {\n        this.showPopup = false;\n      } else {\n        this.showPopup = true;\n      }\n    },\n\n    addRow(e) {\n      const input = document.querySelectorAll('.pop-input');\n      var x = [];\n      input.forEach(function (el) {\n        x.push(el.value);\n      });\n      this.tableData[this.currentPage].Items.push({\n        tableItem: x\n      });\n      localStorage.tableDataFinances = JSON.stringify(this.tableData);\n      this.showPopup = false;\n    },\n\n    removeRow(item) {\n      this.tableData[this.currentPage].Items = this.tableData[this.currentPage].Items.filter(p => p.id !== item.id);\n      localStorage.tableDataFinances = JSON.stringify(this.tableData);\n    },\n\n    search(searchValue) {\n      console.log('hui');\n      let tableItems = document.querySelectorAll('.table li');\n      var value = searchValue;\n\n      if (value != '') {\n        tableItems.forEach(function (el) {\n          if (el.innerText.search(value) == -1) {\n            el.classList.add('hide');\n          } else {\n            el.classList.remove('hide');\n          }\n        });\n      } else {\n        tableItems.forEach(function (el) {\n          el.classList.remove('hide');\n        });\n      }\n    }\n\n  },\n\n  mounted() {\n    if (localStorage.tableDataFinances) {\n      this.tableData = JSON.parse(localStorage.tableDataFinances);\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAuCI,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACF,eAAe;AACbC,YAAU,EAAE;AAAEH,YAAF;AAAYC,WAAZ;AAAqBC,WAArB;AAA8BH;AAA9B,GADC;;AAEbK,MAAI,GAAE;AACJ,WAAM;AACJC,WAAK,EAAE,EADH;AAEJC,eAAS,EAAE,KAFP;AAGJC,iBAAW,EAAE,CACX;AACEC,kBAAU,EAAE,CAAC,GAAD,EAAM,OAAN,EAAe,gBAAf,EAAiC,WAAjC,EAA8C,WAA9C;AADd,OADW,CAHT;AAQJC,eAAS,EAAE,CACT;AACEC,aAAK,EAAE,CACL;AACEC,mBAAS,EAAE,CAAC,GAAD,EAAM,GAAN,EAAW,MAAX,EAAmB,MAAnB,CADb;AACyCC,YAAE,EAAE;AAD7C,SADK,EAIL;AACED,mBAAS,EAAE,CAAC,GAAD,EAAM,IAAN,EAAY,MAAZ,EAAoB,OAApB,CADb;AAC2CC,YAAE,EAAC;AAD9C,SAJK;AADT,OADS,CARP;AAuBJC,WAAK,EAAE,EAvBH;AAwBJC,iBAAW,EAAE;AAxBT,KAAN;AA0BD,GA7BY;;AA8BbC,SAAO,EAAC;AAENC,WAAO,CAACF,WAAD,EAAa;AAClB,WAAKA,WAAL,GAAmBA,WAAnB;AACD,KAJK;;AAMNG,mBAAe,GAAE;AACf,UAAI,KAAKX,SAAL,IAAkB,IAAtB,EAA2B;AACzB,aAAKA,SAAL,GAAiB,KAAjB;AACD,OAFD,MAEK;AACH,aAAKA,SAAL,GAAiB,IAAjB;AACF;AACD,KAZK;;AAaNY,UAAM,CAACC,CAAD,EAAG;AACP,YAAMC,KAAI,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,YAA1B,CAAd;AACA,UAAIC,IAAI,EAAR;AACAH,WAAK,CAACI,OAAN,CAAc,UAASC,EAAT,EAAY;AACxBF,SAAC,CAACG,IAAF,CAAOD,EAAE,CAACZ,KAAV;AACD,OAFD;AAGA,WAAKJ,SAAL,CAAe,KAAKK,WAApB,EAAiCJ,KAAjC,CAAuCgB,IAAvC,CAA4C;AAACf,iBAAS,EAAEY;AAAZ,OAA5C;AACAI,kBAAY,CAACC,iBAAb,GAAiCC,IAAI,CAACC,SAAL,CAAe,KAAKrB,SAApB,CAAjC;AACA,WAAKH,SAAL,GAAiB,KAAjB;AAED,KAvBK;;AAyBNyB,aAAS,CAACC,IAAD,EAAM;AACb,WAAKvB,SAAL,CAAe,KAAKK,WAApB,EAAiCJ,KAAjC,GAAyC,KAAKD,SAAL,CAAe,KAAKK,WAApB,EAAiCJ,KAAjC,CAAuCuB,MAAvC,CAA8CC,KAAKA,CAAC,CAACtB,EAAF,KAASoB,IAAI,CAACpB,EAAjE,CAAzC;AACAe,kBAAY,CAACC,iBAAb,GAAiCC,IAAI,CAACC,SAAL,CAAe,KAAKrB,SAApB,CAAjC;AAED,KA7BK;;AAgCN0B,UAAM,CAACC,WAAD,EAAa;AACjBC,aAAO,CAACC,GAAR,CAAY,KAAZ;AACA